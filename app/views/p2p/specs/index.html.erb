


<% content_for :pagetitle  do %>
    Specifications  -Admin 
<%end %>

<h1>Listing p2p_specs</h1>

<%= link_to '<h5><i class="icon-plus-sign-alt"></i> New Specification</h5>'.html_safe, new_p2p_spec_path %>



<table class="table table-striped table-bordered">
  <tr>
    <th>Name</th>
    
    <th>Priority</th>
    <th>Category</th>
    <th>Action</th>
  </tr>

<% @p2p_specs.each do |p2p_spec| %>
  <tr>
    <td><%= p2p_spec.name %></td>
    <td><%= p2p_spec.priority %></td>
    <td><%= p2p_spec.category.name %></td>

    <td><%= link_to '<i class="icon-edit"></i>'.html_safe, edit_p2p_spec_path(p2p_spec) %>
        <%= link_to '<i class="icon-trash"></i>'.html_safe, p2p_spec, method: :delete, data: { confirm: 'Are you sure?' } %>
    </td>

  </tr>
<% end %>
</table>

<br />


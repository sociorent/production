<%= simple_form_for(@p2p_category) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

 
  	<%= f.input :name%>
 	<%= f.input :priority %>

  	<%= f.input :category_id,  :collection => P2p::Category.all.collect{ |p| [ p.name , p.id]}%>


  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
